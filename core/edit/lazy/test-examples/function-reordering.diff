class Calculator {
  constructor() {
    this.result = 0;
  }

  add(a, b) {
    this.result = a + b;
    return this;
  }

  subtract(a, b) {
    this.result = a - b;
    return this;
  }

  multiply(a, b) {
    this.result = a * b;
    return this;
  }

  divide(a, b) {
    if (b === 0) throw new Error('Division by zero');
    this.result = a / b;
    return this;
  }

  getResult() {
    return this.result;
  }
}

